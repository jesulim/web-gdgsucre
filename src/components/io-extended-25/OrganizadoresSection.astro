---
import { getOrganizers } from "@/lib/services/organizersService"
import { supabase } from "@/lib/supabase"
import { OrganizadoresCarousel } from "./organizadores/organizadoresCard"

const organizers = await getOrganizers(supabase, "io-extended-25")
---

<section id="organizadores" class="flex flex-col items-center py-12">
  <h2 class="text-2xl md:text-4xl font-bold text-black leading-tight text-center pb-4 md:pb-10">
    ORGANIZADORES
  </h2>
  <OrganizadoresCarousel client:visible organizers={organizers} />
</section>
